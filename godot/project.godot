#window/size/viewport_height=384
#window/size/window_width_override=768
#window/size/window_height_override=1152
window/size/always_on_top=true
window/stretch/mode="canvas_items"
window/size/viewport_width=855
window/size/viewport_height=481
window/size/window_width_override=855
