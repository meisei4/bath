; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="bath"
run/main_scene="uid://l5cvget1ulog"
config/project_settings_override="res://experimental_resolution_override.cfg"
config/features=PackedStringArray("4.4", "Forward Plus")

[audio]

driver/enable_input=true

[autoload]

ResolutionManager="*res://Autoloads/ResolutionManager.gd"
AudioEffectManager="*res://Autoloads/AudioEffectManager.gd"
AudioPoolManager="*res://Autoloads/AudioPoolManager.gd"
AudioBus="*res://Audio/AudioBus.gd"
MusicDimensionsManager="*res://Autoloads/MusicDimensionsManager.gd"
ComponentsManager="*res://Autoloads/CharacterComponentsManager.gd"
MechanicManager="*res://Autoloads/MechanicManager.gd"
ComputeShaderSignalManager="res://Autoloads/ComputeShaderSignalManager.gd"
FragmentShaderSignalManager="*res://Autoloads/FragmentShaderSignalManager.gd"
SpacetimeManager="*res://Autoloads/SpacetimeManager.gd"
InterpolationUtil="*res://Mechanics/InterpolationUtil.gd"
RustUtilSingleton="*res://Autoloads/RustUtilSingleton.gd"

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
gdscript/warnings/unsafe_property_access=1
gdscript/warnings/unsafe_method_access=1
gdscript/warnings/unsafe_cast=1
gdscript/warnings/unsafe_call_argument=1

[display]

